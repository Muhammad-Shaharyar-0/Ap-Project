            <p class="text-center booking">Client Details</p>
            <div class="container rectangle client">
                    <p class="divided">
                        <span class="labelHeading">Date </span>
                        <span class="divider"></span>
                        <span class="labelData">May 28,2019</span>
                    </p>
                    <p class="divided">
                        <span class="labelHeading">Appt Time</span>
                        <span class="divider"></span>
                        <span  class="labelData">11 AM</span>
                        </p>
                    <p class="divided">
                        <span class="labelHeading">Child Turns</span>
                        <span class="divider"></span>
                        <span class="labelData">7 Years Old</span>
                    </p>
                    <p class="divided">
                        <span class="labelHeading">Child Name</span>
                        <span class="divider"></span>
                        <span class="labelData">Vincent Pearcy</span>
                    </p>
                    <p class="divided">
                        <span class="labelHeading">Parent Name</span>
                        <span class="divider"></span>
                        <span class="labelData">Marisa Pearcy</span>
                    </p>
                    <p class="divided">
                        <span class="labelHeading">Email</span>
                        <span class="divider"></span>
                        <span class="labelData">tk@gmail.com</span>
                    </p>
                    <p class="divided ">
                        <span class="labelHeading">Phone</span>
                        <span class="divider"></span>
                        <span class="labelData">298-938-2834</span>
                    </p>
                    <br>
                
                               
            </div>
        </div>
    </div>
</div>
</div>
