    <title>Upcoming Booked Parties</title>
    <style>
    .calendar{
        width: 100% !important;
        text-align: center;
        margin-top: -10px;
        padding: 0px 20px 20px 20px;
    }      
    .calendar--days span, .muted {
    border: 1px solid #dadada;
    margin-left: 3px;
    /* border-radius: 3px; */
}
.muted{
    background-color: #FF8100;
    color: white !important;
    border: none;
}
.calendar--days span{
    color: #dadada;
}
    .divided {
  display: flex;
  align-items: center;
  padding-top: 11px;
}

.divider {
  flex-grow: 1;
  border-bottom: 1px solid lightgray;
  margin: 5px
}
.labelHeading{
    color: #FF8100;
    font-size: 14px;
}
.labelData{
    color: black;
    font-size: 14px;
}
    </style>
</head>
<body id="themeBg">
        
<!--theme header-->
<div class="container ">
    <div class="row mt-5">
        <div class="col-12 col-lg-12 col-xl-12 venue ">
           <h4 class="title" style="font-weight: bold;">Upcoming Booked Parties</h4>
        </div>
    </div>
    
<!--2 column area-->
<div class="container">
    <div class="row justify-content-center">
        <!--Calendar-->
        <div class="col-12 col-xl-4 col-lg-4 col-md-6 col-sm-12 col-xs-12 mt-4">
            <p class=" booking text-center">Choose a Booked Date</p>
            <div id="calendar" class=" "  style="background-color: white;  box-shadow:10px 10px 40px rgb(236, 236, 236); " ></div>
        </div>
        <!--Client Details-->
